    "locale": "^0.1.0",
    "prettier": "^1.15.3",
    "puppeteer": "^1.11.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "request-promise": "^4.2.2",
    "unfetch": "^4.0.1"
  },
  "engines": {
    "node": ">= 8.9.0"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          "@babel/preset-flow",
          "@babel/preset-react"
        ],
        "plugins": [
          "@babel/plugin-transform-modules-commonjs"
        ]
      }
    }
