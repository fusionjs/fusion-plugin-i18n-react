{
  "name": "fusion-plugin-i18n-react",
  "version": "1.2.3",
  "description": "Adds I18n (Internationalization) support to a FusionJS app",
  "repository": "fusionjs/fusion-plugin-i18n-react",
  "files": [
    "dist",
    "singleton.js",
    "src"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.es.js",
