  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "create-universal-package": "3.4.6",
    "enzyme": "^3.8.0",
    "eslint": "^5.11.1",
